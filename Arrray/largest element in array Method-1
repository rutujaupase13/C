#include <stdio.h>

int main() {
   int n , largest , arr[100];
    printf("Enter Number of elements:");
    scanf("%d",&n);
    for(int i = 0;i< n; i++){
        printf("Enter elements:");
        scanf("%d",&arr[i]); 
    }
    largest = arr[0];
    for(int j = 0; j< n;j++){
        if(arr[j] > largest)
           largest = arr[j];
    }
    printf("Largest element is %d", largest);

    return 0;
}
