#include <stdio.h>

int main() {
    int n, arr[100];
    printf("Enter the number elements in the array from 1-100:");
    scanf("%d",&n);
    for(int i = 0;i< n;i++){
        scanf("%d",&arr[i]);
    }
    printf("Reverse of the array is:");
    for(int j = n-1 ;j>=0;j--){
        printf("%d",arr[j]);
    }
    return 0;
}
