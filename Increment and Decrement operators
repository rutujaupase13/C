// Arithmetic Operators
#include<stdio.h>

int main()
{
    int a = 10;
    printf("a =%d",a); // a=10
    //increment operators
    printf("a++ = %d\n",a++); // a++ = 10
    printf("++a = %d\n",++a); // ++a = 12
    //decrement operators
    printf("--a = %d\n",--a); //--a = 11
    printf("a-- = %d\n",a--); //a-- = 11
    printf("a =%d\n",a); //a= 10
}
