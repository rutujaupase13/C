#include <stdio.h>
    
    int fact(int x){
        int f =1;
        for(int i=1;i<=x;i++){
            f = f*i;
        }
        return f;
    }
    

    int  main(){
        int n,r;
         printf("Enter value of n:");
         scanf("%d",&n);
         printf("Enter value of r:");
         scanf("%d",&r);
         fact(n);
         fact(n-r);
         int ncr = fact(n)/fact(n-r);
          printf("%d",ncr);
       
         return 0;
    }
